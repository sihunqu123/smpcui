    "test": "ng test",
    "lint": "eslint --ext ts,js,jsx .",
    "lint2": "eslint --print-config .eslintrc.json",
    "fix": "eslint --ext ts,js,jsx,json --fix src",
    "e2e": "ng e2e"
  },
  "private": true,
