  "scripts": {
    "ng": "ng",
    "start": "ng serve --allowed-hosts 'ubuntutc'  --host '0.0.0.0' --port '4201' --disable-host-check",
    "start-win": "ng serve --allowed-hosts 'ubuntutc'  --host 0.0.0.0 --port 4201 --disable-host-check",
